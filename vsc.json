{
    "files.autoSave": "afterDelay",
    "svg.preview.mode": "svg",
    "explorer.sortOrder": "type",
    "editor.bracketPairColorization.enabled": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.semanticHighlighting.enabled": false,
    "betterAlign.surroundSpace": {
      "colon": [
        1,
        1
      ], // The first number specify how much space to add to the left, can be negative.
      "assignment": [
        1,
        1
      ], // The same as above.
      "arrow": [
        1,
        1
      ], // The same as above.
      "comment": 2 // Special how much space to add between the trailing comment and the code.
    },
    "editor.formatOnSave": true,
    "emmet.includeLanguages": {
      "rust": "html"
    },
    "editor.tokenColorCustomizations": {
      "textMateRules": [
        // STRINGS
        {
          "name": "string",
          "scope": [
            "string.quoted.double",
            "string.quoted.single",
            "punctuation.definition.string.begin",
            "punctuation.definition.string.end",
            "string.quoted.multi",
            "string.regexp",
            "string.quoted.raw",
            "support.constant.color.w3c-standard-color-name.css",
            "support.constant.property-value.css",
            "keyword.other.unit.px.css",
            "constant.other.color.rgb-value.hex.css",
            "keyword.other.unit.percentage.css",
            "string.quoted.double.rust",
            "punctuation.definition.string.rust",
          ],
          "settings": {
            "foreground": "#51c48f"
          }
        },
        {
          "name": "string-key",
          "scope": [
            "support",
            "punctuation.support"
          ],
          "settings": {
            "foreground": "#dddddd"
          }
        },
        {
          "name": "string-key",
          "scope": [
            "constant.character.escape"
          ],
          "settings": {
            "foreground": "#828282"
          }
        },
        // KEYWORD
        {
          "name": "keyword",
          "scope": [
            "keyword.control",
            "variable.language.special",
            "entity.name.tag.css",
            "entity.name.tag",
            "storage.type.rust"
          ],
          "settings": {
            "foreground": "#64a3f5"
          }
        },
        // function
        {
          "name": "functions",
          "scope": [
            "entity.name.function",
            "support.function",
            "entity.name.type",
            "meta.function-call.generic",
            "support.function.misc.css",
            "support.constant.color.w3c-standard-color-name",
            "entity.other.attribute-name.html",
            "entity.other.attribute-name.id.css",
            "punctuation.definition.entity.css",
            "entity.other.attribute-name.class.css"
          ],
          "settings": {
            "foreground": "#d068ff"
          }
        },
        // TYPE
        {
          "name": "type",
          "scope": [
            "storage.type",
            "support.type",
            "storage.type.string",
            "entity.other.inherited-class",
            "support.function.misc.css",
            "keyword.other.important.css",
            "entity.name.type.struct.rust",
            "entity.name.type"
          ],
          "settings": {
            "foreground": "#FE5454"
          }
        },
        // operator
        {
          "name": "operator",
          "scope": [
            "keyword.operator",
            "keyword.operator.logical.python",
            "entity.other.attribute-name.pseudo-class.css",
            "entity.other.attribute-name.pseudo-element.css",
            "support.constant.font-name.css"
          ],
          "settings": {
            "foreground": "#ffef60"
          }
        },
        // ponctuation
        {
          "name": "ponctuation",
          "scope": [
            "punctuation.definition",
            "punctuation.definition.entity.css",
            "meta.attribute.rust"
          ],
          "settings": {
            "foreground": "#888888"
          }
        },
        // ponctuation
        {
          "name": "ponctuation",
          "scope": [
            "punctuation"
          ],
          "settings": {
            "foreground": "#828282"
          }
        },
        // word
        {
          "name": "word",
          "scope": [
            "source",
            "constant.character.format.placeholder.other.python",
            "meta.fstring.python",
            "variable",
            "variable.other.constant",
            "variable.other.object.js",
            "text.html",
            "variable.other.object",
            "constant.character.set.regexp",
            "constant.other.set.regexp",
            "support.type.property-name",
            "punctuation.separator.period"
          ],
          "settings": {
            "foreground": "#e3e3e3"
          }
        },
        {
          "name": "word",
          "scope": [
            "support.type.vendored.property-name.css"
          ],
          "settings": {
            "foreground": "#e3e3e3",
            "fontStyle": "italic"
          }
        },
        // in
        {
          "name": "int",
          "scope": [
            "constant.numeric",
            "constant.language"
          ],
          "settings": {
            "foreground": "#51c48f",
            "fontStyle": "italic",
          }
        },
        // comments
        {
          "name": "com",
          "scope": [
            "comment",
            "punctuation.definition.comment"
          ],
          "settings": {
            "foreground": "#444444"
          }
        }
      ]
    },
